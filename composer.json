{
    "autoload": {
        "psr-4": {
            "App\\": "src/app/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://bitbucket.org/archon810/smartdomdocument.git"
        }
    ],
    "require": {
        "php": ">=7",
        "archon810/smartdomdocument": "^0.4.2",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/plates": "^3.3",
        "mikecao/flight": "^1.3.8",
        "riimu/kit-pathjoin": "^1.2",
        "symfony/finder": "^4.0"
    },
    "config": {
        "process-timeout": 3000,
        "sort-packages": true,
        "vendor-dir": "vendor"

    },
    "require-dev": {
        "bramus/monolog-colored-line-formatter": "^3.0",
        "filp/whoops": "^2.9",
        "joshtronic/php-loremipsum": "dev-master",
        "kint-php/kint": "~3.0",
        "monolog/monolog": "^1.23",
        "psy/psysh": "@stable"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
